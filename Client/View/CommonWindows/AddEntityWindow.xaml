        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client.View.CommonWindows"
        mc:Ignorable="d"
        Title="AddEntityWindow" Height="300" Width="300">
    <Grid>
        <StackPanel x:Name="Elements" HorizontalAlignment="Left" Height="220" VerticalAlignment="Top" Width="294" Margin="0,0,-0.4,0"/>
        <Button x:Name="okButton" Content="Ok" HorizontalAlignment="Left" Margin="55,240,0,0" VerticalAlignment="Top" Width="75" Click="OkButton_Click"/>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="170,240,0,0" VerticalAlignment="Top" Width="75" Click="CancelButton_Click"/>

    </Grid>
</Window>
